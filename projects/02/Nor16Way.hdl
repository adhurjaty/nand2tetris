/**
 * Adds two 16-bit values.
 * The most significant carry bit is ignored.
 */

CHIP Nor16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way(in=in[0..7], out=o1);
    Or8Way(in=in[8..15], out=o2);
    Or(a=o1, b=o2, out=o);
    Not(in=o, out=out);
}